	<jobSpace>
		<job name="SHOP_ORDER" source="HANA_PROD" target="HANA_ARCHIVE_TEST" itemRunner="ui.tmd.zztop.TxKeyItemRunner">
			<item taskName="SHOP_ORDER" where="SITE='1020' and ACTUAL_COMP_DATE is not null and now()>add_days(ACTUAL_COMP_DATE, 140)" />
		</job>
		<job name="SHOP_ORDER_DELETE" source="HANA_ARCHIVE_TEST" sourceDelete="true" target="IDLE" itemRunner="ui.tmd.zztop.TxKeyItemRunner">
			<item taskName="SHOP_ORDER" where="SITE='1020' and ACTUAL_COMP_DATE is not null and now()>add_days(ACTUAL_COMP_DATE, 140)" />
		</job>
		<job name="PKG_CONTAINER" source="HANA_PROD" target="HANA_ARCHIVE_TEST" desc="" itemRunner="ui.tmd.zztop.TxKeyItemRunner">
			<item taskName="ZR_PKG_CONTAINER" where="STATE_NAME in ('IN_STOCK') and now()>add_days(PACKING_TIME, 100)" />
		</job>
		<job name="PMS_LOG" source="HANA_TEST" target="PGSVR" itemRunner="ui.tmd.zztop.TxTableItemRunner">
			<item taskName="Z_PMS_JOB_OPERATION_LOG">
				<args>
					<arg>CREATED_DATE_TIME</arg>
				</args>
			</item>
			<item taskName="Z_PMS_RESOURCE_MOVE_LOG" enabled="false">
				<args>
