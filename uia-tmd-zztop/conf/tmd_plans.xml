				</args>
			</item>
		</job>
		<job name="PMS_TX" source="HANA_TEST" target="PGSVR" desc="" itemRunner="ui.tmd.zztop.TxHistoryItemRunner">
			<item taskName="Z_PMS_JOB" where="JOB_STATUS='COMPLETE'">
				<args>
					<arg>COMPLETE_DATE_TIME</arg>
					<arg>4</arg>
				</args>
			</item>
			<item taskName="Z_PMS_PLAN_CHANGE" where="STATUS in ('APPROVE','NOT_USE','REJECT')">
				<args>
					<arg>UPDATE_TIME</arg>
					<arg>12</arg>
				</args>
			</item>
		</job>
